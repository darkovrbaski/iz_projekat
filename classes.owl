#################################################################
#    Data properties
#################################################################

###  https://github.com/darkovrbaski/iz_projekat/master/classes.owl#wireless
:wireless rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Keyboard
                                      :Mouse
                                      :Speakers
                                    )
                      ] ;
          rdfs:range xsd:boolean .


