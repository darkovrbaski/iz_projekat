#################################################################
#    Object Properties
#################################################################

###  https://github.com/darkovrbaski/iz_projekat/master/classes.owl#hasBrand
:hasBrand rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Component
                                      :PersonalComputer
                                    )
                      ] ;
          rdfs:range :Brand .


###  https://github.com/darkovrbaski/iz_projekat/master/classes.owl#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :PersonalComputer ;
              rdfs:range :Component .


###  https://github.com/darkovrbaski/iz_projekat/master/classes.owl#hasCoolingType
:hasCoolingType rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Cooling ;
                rdfs:range :CoolingType .


###  https://github.com/darkovrbaski/iz_projekat/master/classes.owl#hasKeyType
:hasKeyType rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Keyboard ;
            rdfs:range :KeyType .


