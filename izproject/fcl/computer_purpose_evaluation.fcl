	RULE 71	: IF number_of_cores IS medium AND single_core_clock IS high AND ram_size IS medium AND v_ram_size IS high  THEN hosting IS low;
	RULE 72	: IF number_of_cores IS medium AND single_core_clock IS high AND ram_size IS high AND v_ram_size IS low  THEN hosting IS low;
	RULE 73	: IF number_of_cores IS medium AND single_core_clock IS high AND ram_size IS high AND v_ram_size IS medium  THEN hosting IS low;
	RULE 74	: IF number_of_cores IS medium AND single_core_clock IS high AND ram_size IS high AND v_ram_size IS high  THEN hosting IS low;

