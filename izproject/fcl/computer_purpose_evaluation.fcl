	RULE 18	: IF number_of_cores IS medium AND single_core_clock IS low AND ram_size IS low AND v_ram_size IS low THEN home_use IS medium;
	RULE 19	: IF number_of_cores IS medium AND single_core_clock IS low AND ram_size IS medium AND v_ram_size IS low THEN home_use IS medium;
	RULE 20	: IF number_of_cores IS medium AND single_core_clock IS medium AND ram_size IS low AND v_ram_size IS low THEN home_use IS high;
	RULE 21	: IF number_of_cores IS medium AND single_core_clock IS medium AND ram_size IS medium AND v_ram_size IS low THEN home_use IS high;

