	RULE 54	: IF number_of_cores IS medium AND single_core_clock IS high AND ram_size IS high AND v_ram_size IS high  THEN gaming IS one;
	
	RULE 55	: IF number_of_cores IS high AND single_core_clock IS medium AND ram_size IS medium AND v_ram_size IS medium  THEN gaming IS high;
