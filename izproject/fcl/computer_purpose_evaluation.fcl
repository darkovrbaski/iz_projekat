FUNCTION_BLOCK computer_purpose_evaluation	// Block definition (there may be more than one block per file)

VAR_INPUT				// Define input variables
	number_of_cores : REAL;
	single_core_clock : REAL;
	ram_size : REAL;
	v_ram_size : REAL;
END_VAR

VAR_OUTPUT				// Define output variable
	home_use : REAL;
	gaming : REAL;
	mining : REAL;
	hosting : REAL;
END_VAR

FUZZIFY number_of_cores
	TERM low := (1,1) (2,1) (3,0);
	TERM medium := (2,0) (8,1) (12,0);
	TERM high := (10,0) (128,1);
END_FUZZIFY

FUZZIFY single_core_clock
	TERM low := (1.0,1) (2.0,1) (3.0,0);
	TERM medium := (2.5,0) (3.5,1) (4.5,0);
	TERM high := (4.2,0) (6.0,1);
END_FUZZIFY

FUZZIFY ram_size
	TERM low := (1,1) (4,1) (6,0);
	TERM medium := (4,0) (16,1) (32,0);
	TERM high := (28,0) (128,1);
END_FUZZIFY

FUZZIFY v_ram_size
	TERM low := (0.5,1) (0.8,1) (3,0);
	TERM medium := (2,0) (4,1) (8,0);
	TERM high := (6,0) (48,1);
END_FUZZIFY

DEFUZZIFY home_use
	TERM zero := (-0.1,0) (0,1) (0.1,0);
	TERM low := (0,0) (0.1,1) (0.5,0);
	TERM medium := (0.3,0) (0.5,1) (0.7,0);
	TERM high := (0.5,0) (0.9,1) (1,0);
	TERM one := (0.9,0) (1,1) (1.1,0);
	METHOD : COG;		// Use 'Center Of Gravity' defuzzification method
	DEFAULT := 0;		// Default value is 0 (if no rule activates defuzzifier)
END_DEFUZZIFY

DEFUZZIFY gaming
	TERM zero := (-0.1,0) (0,1) (0.1,0);
	TERM low := (0,0) (0.1,1) (0.5,0);
	TERM medium := (0.3,0) (0.5,1) (0.7,0);
	TERM high := (0.5,0) (0.9,1) (1,0);
	TERM one := (0.9,0) (1,1) (1.1,0);
	METHOD : COG;		// Use 'Center Of Gravity' defuzzification method
	DEFAULT := 0;		// Default value is 0 (if no rule activates defuzzifier)
END_DEFUZZIFY

DEFUZZIFY mining
	TERM zero := (-0.1,0) (0,1) (0.1,0);
	TERM low := (0,0) (0.1,1) (0.5,0);
	TERM medium := (0.3,0) (0.5,1) (0.7,0);
	TERM high := (0.5,0) (0.9,1) (1,0);
	TERM one := (0.9,0) (1,1) (1.1,0);
	METHOD : COG;		// Use 'Center Of Gravity' defuzzification method
	DEFAULT := 0;		// Default value is 0 (if no rule activates defuzzifier)
END_DEFUZZIFY

DEFUZZIFY hosting
	TERM zero := (-0.1,0) (0,1) (0.1,0);
	TERM low := (0,0) (0.1,1) (0.5,0);
	TERM medium := (0.3,0) (0.5,1) (0.7,0);
	TERM high := (0.5,0) (0.9,1) (1,0);
	TERM one := (0.9,0) (1,1) (1.1,0);
	METHOD : COG;		// Use 'Center Of Gravity' defuzzification method
	DEFAULT := 0;		// Default value is 0 (if no rule activates defuzzifier)
END_DEFUZZIFY

RULEBLOCK No1
	AND : MIN;			// Use 'min' for 'and' (also implicit use 'max' for 'or' to fulfill DeMorgan's Law)
	ACT : MIN;			// Use 'min' activation method
	ACCU : MAX;			// Use 'max' accumulation method

	// General rules
	RULE 1 : IF number_of_cores IS low THEN gaming IS low;
	RULE 2 : IF number_of_cores IS low THEN hosting IS zero;
	
	RULE 3 : IF single_core_clock IS low THEN gaming IS low;
	RULE 4 : IF single_core_clock IS low THEN hosting IS low;

	RULE 5 : IF ram_size IS low THEN gaming IS low;
	RULE 6 : IF ram_size IS low THEN mining IS low;
	RULE 7 : IF ram_size IS low THEN hosting IS zero;
	
	RULE 8 : IF v_ram_size IS low THEN gaming IS low;
	RULE 9 : IF v_ram_size IS low THEN mining IS low;
	
	// Home use rules
	RULE 10 : IF number_of_cores IS low AND single_core_clock IS low AND ram_size IS low AND v_ram_size IS low THEN home_use IS zero;
	
	RULE 11	: IF number_of_cores IS low AND single_core_clock IS low AND ram_size IS medium AND v_ram_size IS low  THEN home_use IS high;
	RULE 12	: IF number_of_cores IS low AND single_core_clock IS medium AND ram_size IS low AND v_ram_size IS low  THEN home_use IS one;
	RULE 13	: IF number_of_cores IS low AND single_core_clock IS medium AND ram_size IS medium AND v_ram_size IS low  THEN home_use IS high;
	
	RULE 14	: IF number_of_cores IS medium AND single_core_clock IS low AND ram_size IS low AND v_ram_size IS medium  THEN home_use IS medium;
